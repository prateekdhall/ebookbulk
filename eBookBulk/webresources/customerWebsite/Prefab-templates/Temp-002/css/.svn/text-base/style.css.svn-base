@charset "utf-8";
/* CSS Document */
*{margin:0px; padding:0px}body, html{ width:100%; height:100%; font-family:Tahoma; font-size:13px; color:#000; }
img{ border:0px; color:#FFF;} a{text-decoration:none;} h1{font-size:17px;color:#000;font-weight:bold;margin:0 0 6px 0;padding:0;text-transform:uppercase} .mandatory{color:#F00;}p{font-size:12px;line-height:20px;margin-bottom:6px;} 
h2{font-size:15px;color:#000;font-weight:bold;margin:0 0 6px 0;padding:0;text-transform:uppercase}
.wrapper{ width:920px; background-color:#B4D6A3;margin:0px auto; position:relative;}
#header{background:url('../images/header1px.jpg')  repeat-x scroll 0 0 #B4D6A3; border: 3px solid #DFCA99; height: 115px;margin-bottom: 5px; padding: 3px;
    position: absolute; width: 909px;} .logo{position:absolute;margin:8px 10px;left:0px;top:46px;} .logo h1{ font-size:23px;!important;margin:0 0 2px 0;} .logo-icon{position:absolute;width:150px;left:37px;margin:2px 10px;}
.register{ background: none repeat scroll 0 0 #FFF; border: 3px solid #DFCA99; min-height:402px; padding: 8px; position: absolute;
    right: 0; top: 135px;  width: 225px;height:auto !important;}
.tradingprice{ background: none repeat scroll 0 0 #FFF; border: 3px solid #DFCA99; min-height:402px; height:auto !important; padding: 8px; position: absolute;  top: 135px;  width: 645px;}
#bookCover{border:1px solid #ccc; padding:4px;width:100px;height:155px;float:left;margin:0 15px 10px 0; background-color:#FFF;}
select{margin-bottom:15px;min-width:225px;font-size:11px; border:1px solid #DFCA99} input[type=text]{margin-bottom:12px;min-width:220px; border:1px solid #DFCA99}
.button{padding:3px 9px;font-size:11px;color:#fff;border:none;background:#07AD37; background: -moz-linear-gradient(top,  #B4D6A3 0%, #0ADC48 16%, #07AD37 29%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#B4D6A3), color-stop(16%,#E7DAB8), color-stop(29%,#DFCA99)); background: -webkit-linear-gradient(top,  #B4D6A3 0%,#0ADC48 16%,#07AD37 29%); background: -o-linear-gradient(top,  #87e0fd 0%,#0ADC48 16%,#05abe0 29%); background: -ms-linear-gradient(top,  #B4D6A3 0%,#0ADC48 16%,#07AD37 29%); background: linear-gradient(top,  #87e0fd 0%,#53cbf1 16%,#07AD37 29%); margin: 5px 0 1px;cursor:pointer}
#footer{position:absolute;top:568px;width:915px;margin:0 auto;text-align:center; background:url("../images/footer1px.jpg") repeat-x scroll 0 10px #B4D6A3; border:3px solid #DFCA99; min-height:25px;vertical-align:middle;height:auto !important;}

