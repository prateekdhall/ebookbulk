#content #block02 #updates {margin-top:10px;}
#content #block02 #updates ul li{line-height:25px;}
#content #block03, #content #block04{width:600px;float:left;margin:1px 0 3px 0;}
#content #block03 .thumbWrapper,#content #block04 .thumbWrapper{width:615px;border:1px solid #bef0ef;text-align:left;}
