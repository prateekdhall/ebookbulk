header .col2 ul li a{padding:10px 8px;background:#bef0ef; color:#015e75;text-decoration:none;}
header .col2 ul li a:hover{background:#015e75; color:#f2f2f2;text-decoration:none;}
#content{margin-top:4px;width:972px;border:1px solid #f2f2f2;border-right:none; border-left:none;min-height:578px;}
#content #block01{width:600px;border:1px solid #bef0ef;padding:8px;margin:5px 5px 5px 1px;float:left;text-align:left}
#content #block01 h1{font-size:23px; margin:0 0 0 0;color:#006a87;}
