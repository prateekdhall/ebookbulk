	color:#fff;
	text-decoration:none;
}
header #application-bar{
	margin:0 auto;
	width:960px;
}
header #application-bar .pubLogo{
	float:left;
	width:auto;
}
header #application-bar .tabbedNavigation{
	float:right;
}
header #application-bar .tabbedNavigation ul{
	margin:0;
	padding:0;
}
/*Application Menu*/
header #application-bar .tabbedNavigation {
	position:relative;
	top:33px;
}
header #application-bar .tabbedNavigation ul.tabs {
	margin: 0 3px -1px 0;
	overflow: hidden;
	float:right;
}
header #application-bar .tabbedNavigation ul.tabs li {
	float: left;
	list-style: none;
	margin: 0;
	overflow: hidden;
	position: relative;
	z-index: 1;
}
/*#appMenu .tabs li {
	z-index: 3;
}*/
header #application-bar .tabbedNavigation ul.tabs li a {
	float: left;
	font-size:12px;
	border:1px solid #cacaca;
	border-bottom:none;
	-webkit-border-radius: 4px 4px 0 0;
	-moz-border-radius: 4px 4px 0 0;
	border-radius: 4px 4px 0 0;
	padding: 10px 20px 8px;
	font-weight:700;
	color: #3072B3;
	text-decoration: none;
	background: -webkit-gradient(linear, 0 0, 0 95%, from(#dbdbdb), to(#f9f9f9));
	background: -moz-linear-gradient(#dbdbdb, #f9f9f9 95%);
	background: linear-gradient(#dbdbdb, #f9f9f9 95%);
}
header #application-bar .tabbedNavigation ul.tabs li a:hover {
	color:#000;
}

