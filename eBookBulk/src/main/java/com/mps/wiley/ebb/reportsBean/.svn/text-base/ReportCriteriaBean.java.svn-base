package main.java.com.mps.wiley.ebb.reportsBean;

import main.java.com.mps.common.dto.IdValueBean;

/**
 * 
 * @author : Krishna Gaur
 * @category : Reports
 * 
 */

public class ReportCriteriaBean
{
	private Integer projectId;
	
	private String projectName;
	
	private Integer projectType;
	
	private Integer ecomp;
	
	private IdValueBean customer = new IdValueBean();
	
	private IdValueBean requester = new IdValueBean();

	private IdValueBean prjAdmin = new IdValueBean();
	
	private String[] statusArray = new String[] {};
	
	private String createdBy;

	public Integer getProjectId()
	{
		return projectId;
	}

	public void setProjectId(Integer projectId)
	{
		this.projectId = projectId;
	}

	public String getProjectName()
	{
		return projectName;
	}

	public void setProjectName(String projectName)
	{
		this.projectName = projectName;
	}

	public Integer getProjectType()
	{
		return projectType;
	}

	public void setProjectType(Integer projectType)
	{
		this.projectType = projectType;
	}

	public Integer getEcomp()
	{
		return ecomp;
	}

	public void setEcomp(Integer ecomp)
	{
		this.ecomp = ecomp;
	}

	public IdValueBean getCustomer()
	{
		return customer;
	}

	public void setCustomer(IdValueBean customer)
	{
		this.customer = customer;
	}

	public IdValueBean getRequester()
	{
		return requester;
	}

	public void setRequester(IdValueBean requester)
	{
		this.requester = requester;
	}

	public IdValueBean getPrjAdmin()
	{
		return prjAdmin;
	}

	public void setPrjAdmin(IdValueBean prjAdmin)
	{
		this.prjAdmin = prjAdmin;
	}

	public String[] getStatusArray()
	{
		return statusArray;
	}

	public void setStatusArray(String[] statusArray)
	{
		this.statusArray = statusArray;
	}

	public String getCreatedBy()
	{
		return createdBy;
	}

	public void setCreatedBy(String createdBy)
	{
		this.createdBy = createdBy;
	}
	
}
